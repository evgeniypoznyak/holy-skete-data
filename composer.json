{
    "name": "evgeniyp/symfony4",
    "type": "project",
    "require": {
        "symfony/website-skeleton": "dev-master",
        "phpunit/phpunit": "^6@dev",
        "symfony/dotenv": "^4.1@dev",
        "symfony/phpunit-bridge": "^4.1@dev",
        "guzzlehttp/guzzle": "^6.3@dev",
        "symfony/dom-crawler": "^4.1@dev",
        "symfony/apache-pack": "dev-master"
    },
    "authors": [
        {
            "name": "Evgeniy Poznyak",
            "email": "evgeniyp@fandiexpress.com"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/web-server-bundle": "^4.2@dev"
    }
}
